{$R *.RES}
  var hPrevWin: HWND;
      hMutex : THandle;

begin
  Application.Initialize;
  Application.Title := 'Магнат';
// Поиск предыдущего окна
{  hPrevWin := FindWindow('TMain', 'Main');
  if hPrevWin <> 0 then
  begin
    SetForegroundWindow(hPrevWin);
    Application.Terminate;
  end;
}
hMutex := CreateMutex(nil, False, 'Magnat01_05'); 
 if WaitForSingleObject(hMutex, 0) <> wait_TimeOut then 

begin 


  Application.CreateForm(TDataMain, DataMain);
  Application.CreateForm(TRevData, RevData);
  Application.CreateForm(TKlad_Data, Klad_Data);
  Application.CreateForm(TMain, Main);

  Application.Run;
end;
end.
